{
    "Align environment": {
	"prefix": "al",
	"body": [
	    "\\begin{align*}",
	    "\t$0",
	    "\\end{align*}"
	]
    },
    "Inline math": {
	"prefix": "mk",
	"body": [
	    "\\($0\\)"
	],
	"luasnip": {
	    "autotrigger": true
	}
    },
    "Problem environment": {
	"prefix": "pb",
	"body": [
	    "\\begin{problem}",
	    "\t$0",
	    "\\end{problem}"
	]
    },
    "Solution environment": {
	"prefix": "sl",
	"body": [
	    "\\begin{solution}",
	    "\t$0",
	    "\\end{solution}"
	]
    },
    "Subproblem environment": {
	"prefix": "spb",
	"body": [
	    "\\begin{subproblem}",
	    "\t$0",
	    "\\end{subproblem}"
	]
    },
    "General environment": {
	"prefix": "\\begin",
	"body": [
	    "\\begin{$1}",
	    "    $0",
	    "\\end{$1}"
	]
    },
    "Fraction": {
	"prefix": "\\frac",
	"body": [
	    "\\frac{$1}{$2}$0"
	]
    }
}
