{
    "Align environment": {
	"prefix": "al",
	"body": [
	    "\\begin{align*}",
	    "\t$1",
	    "\\end{align*}$0"
	]
    },
    "Inline math": {
	"prefix": "mk",
	"body": [
	    "\\($1\\)"
	]
    },
    "Problem environment": {
	"prefix": "pb",
	"body": [
	    "\\begin{problem}",
	    "\t$1",
	    "\\end{problem}$0"
	]
    },
    "Solution environment": {
	"prefix": "sl",
	"body": [
	    "\\begin{solution}",
	    "\t$1",
	    "\\end{solution}$0"
	]
    },
    "Subproblem environment": {
	"prefix": "spb",
	"body": [
	    "\\begin{subproblem}",
	    "\t$1",
	    "\\end{subproblem}$0"
	]
    },
    "General environment": {
	"prefix": "\\beg",
	"body": [
	    "\\begin{$1}",
	    "    $0",
	    "\\end{$1}"
	]
    },
    "Fraction": {
	"prefix": "\\frac",
	"body": [
	    "\\frac{$1}{$2}$0"
	]
    }
}
